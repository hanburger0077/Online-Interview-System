# 开发环境配置

# 环境标识
VITE_ENV=development

# API 基础地址
VITE_API_BASE_URL=http://localhost:8080/api/v1

# WebSocket 地址
VITE_WS_BASE_URL=ws://localhost:8080/ws

# 是否启用 Mock
VITE_USE_MOCK=true

# 是否启用代理
VITE_USE_PROXY=true

# RTC 提供商（webrtc / agora / tencent）
VITE_RTC_PROVIDER=webrtc

# 是否开启调试模式
VITE_DEBUG=true

# 日志级别（DEBUG / INFO / WARN / ERROR）
VITE_LOG_LEVEL=DEBUG
