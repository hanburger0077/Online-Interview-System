# 生产环境配置

# 环境标识
VITE_ENV=production

# API 基础地址（需要根据实际部署修改）
VITE_API_BASE_URL=https://api.example.com/api/v1

# WebSocket 地址（需要根据实际部署修改）
VITE_WS_BASE_URL=wss://api.example.com/ws

# 是否启用 Mock
VITE_USE_MOCK=false

# 是否启用代理
VITE_USE_PROXY=false

# RTC 提供商
VITE_RTC_PROVIDER=webrtc

# 是否开启调试模式
VITE_DEBUG=false

# 日志级别
VITE_LOG_LEVEL=ERROR

# CDN 地址（可选）
VITE_CDN_URL=https://cdn.example.com
