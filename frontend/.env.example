# 环境变量示例文件
# 复制此文件为 .env.development 或 .env.production 并修改对应的值

# 环境标识
VITE_ENV=development

# API 基础地址
VITE_API_BASE_URL=http://localhost:8080/api/v1

# WebSocket 地址
VITE_WS_BASE_URL=ws://localhost:8080/ws

# 是否启用 Mock
VITE_USE_MOCK=true

# RTC 提供商
VITE_RTC_PROVIDER=webrtc

# RTC 密钥（如使用第三方服务需要填写）
VITE_RTC_APP_ID=
VITE_RTC_APP_KEY=

# 是否开启调试模式
VITE_DEBUG=true

# 日志级别
VITE_LOG_LEVEL=DEBUG

# 日志上报地址（可选）
VITE_LOG_REPORT_URL=
